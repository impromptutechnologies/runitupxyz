(function() {
  var start = new Date;
  start.setUTCHours(14, 30, 0);
  var end = new Date;
  end.setUTCHours(21, 30, 0); // 11pm
  function pad(num) {
    return ("0" + parseInt(num)).substr(-2);
  }
  function tick() {
    
    var date = new Date;
    var now_utc = Date.UTC(date.getUTCFullYear(), date.getUTCMonth(), date.getUTCDate(),
    date.getUTCHours(), date.getUTCMinutes(), date.getUTCSeconds());
    console.log(now_utc, start, end)
    console.log(now_utc > start, now_utc < end, now_utc > end)
    if (now_utc > start && now_utc < end && date.getDay() !== 6 && date.getDay() !== 0) { 
      document.getElementById('time').innerHTML =
      "Market Open. Place commands after market close."
    }
    if(date.getDay() == 6 || date.getDay() == 0){
       document.getElementById('time').innerHTML =
        "Its the weekend. Have some fun :)"
    }
    if(now_utc < start && date.getDay() !== 6){
      var remain = ((start - now_utc) / 1000);
      var hh = pad((remain / 60 / 60) % 60);
      var mm = pad((remain / 60) % 60);
      var ss = pad(remain % 60);
      document.getElementById('time').innerHTML = "Place your Command";
    }
    
     
    
    setTimeout(tick, 1000);
  }
  document.addEventListener('DOMContentLoaded', tick);
})();

</script>

